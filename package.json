{
  "name": "lightdm-webkit-aether",
  "version": "1.0.0",
  "description": "Elegant LightDM Webkit theme.",
  "main": "index.js",
  "scripts": {
    "test": "./node_modules/.bin/eslint src/es6/**",
    "build": "npm run test && ./node_modules/.bin/babel src/es6 --out-dir src/js && ./node_modules/.bin/node-sass src/sass --output src/css",
    "watch": "./node_modules/.bin/babel src/es6 --watch --out-dir src/js & ./node_modules/.bin/node-sass --watch src/sass --output src/css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/noisek/lightdm-webkit-aether.git"
  },
  "keywords": [
    "webkit",
    "aether",
    "lightdm",
    "login",
    "manager",
    "archlinux",
    "arch"
  ],
  "author": "Noi Sek",
  "license": "GPLv2",
  "bugs": {
    "url": "https://github.com/noisek/lightdm-webkit-aether/issues"
  },
  "homepage": "https://github.com/noisek/lightdm-webkit-aether#readme",
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-cli": "^6.10.1",
    "babel-plugin-inferno": "^0.7.3",
    "babel-plugin-transform-es2015-modules-amd": "^6.8.0",
    "babel-preset-es2015": "^6.9.0",
    "eslint": "^3.0.1",
    "eslint-plugin-react": "^5.2.2",
    "inferno": "^0.7.13",
    "inferno-dom": "^0.7.13",
    "node-sass": "^3.8.0"
  }
}
